{
  "name": "leaflet-control-angular",
  "version": "0.1.1",
  "description": "Allows for the use of AngularJS in custom leaflet controls.",
  "dependencies": {
    "angular": "^1",
    "leaflet": "^0.7.7"
  },
  "main": "src/L.Control.Angular.js",
  "scripts": {
  },
  "eslintConfig": {
    "root": true,
    "globals": {
      "L": true
    },
    "env": {
      "commonjs": true,
      "amd": true,
      "node": false
    },
    "rules": {
      "no-mixed-spaces-and-tabs": [2, "smart-tabs"],
      "indent": [2, "tab", {"VariableDeclarator": 0}],
      "curly": 2,
      "spaced-comment": 2,
      "strict": 0,
      "wrap-iife": 0,
      "key-spacing": 0,
      "consistent-return": 0
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/grantHarris/leaflet-control-angular.git"
  },
  "keywords": [
    "gis",
    "map",
    "angular",
    "leaflet"
  ],
  "license": "MIT"
}